<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
    <title>@ViewBag.Title</title>
    <link href="@Url.Content("~/Content/Site.css")" rel="stylesheet" type="text/css" />
    <script src="@Url.Content("~/Scripts/jquery-1.5.1.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/modernizr-1.7.min.js")" type="text/javascript"></script>
    
    <link media="screen, projection" type="text/css" href="@Url.Content("~/Content/media/css/screen.css")" rel="stylesheet">
	<link media="print" type="text/css" href="@Url.Content("~/Content/media/css/print.css")" rel="stylesheet">
	<link type="text/css" href="@Url.Content("~/Content/media/css/main.css")" rel="stylesheet">
	<link type="text/css" href="@Url.Content("~/Content/media/css/plugins/buttons/screen.css")" rel="stylesheet">
</head>

<body>
<div class="container">
		
			
<header>
				<h1>@ViewBag.Title</h1>
			</header>
			
			<div class="span-24 main_content">
    @RenderBody()
            <footer>
				<p>&copy; 2011</p>
			</footer>
		
		</div>
</body>
</html>
